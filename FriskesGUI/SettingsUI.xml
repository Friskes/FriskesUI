      <Button name="$parentNETButton" inherits="OptionsButtonTemplate" text="net">
        <Size x="19" y="20" />
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="67" y="-11.5" />
            </Offset>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnEnter>
            GameTooltip:SetOwner(self, "ANCHOR_RIGHT")
            GameTooltip:SetText("Use: Shift+Left, Alt+Left, Ctrl+Left.", 1, 0.82, 0, 1, false)
          </OnEnter>
          <OnLeave>
            GameTooltip:SetText("", 1, 0.82, 0, 1, false)
          </OnLeave>
          <OnClick>
            local argument
            if ( button == "LeftButton" ) then
                if ( IsShiftKeyDown() ) then
                    argument = 35
                elseif ( IsAltKeyDown() ) then
                    argument = 65
                elseif ( IsControlKeyDown() ) then
                    argument = 100
                else
                    argument = 1
                end
            end
            FriskesUI_Net_Toggle(argument)
          </OnClick>
        </Scripts>
      </Button>
